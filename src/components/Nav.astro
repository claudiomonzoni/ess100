</style>

<script>
  // -------------------------------------------------
  //  Variables
  // -------------------------------------------------
  let lastScroll = 0;
  const nav = document.querySelector(".nav-main");
  const hamburger = document.querySelector(".hamburger");
  const navContent = document.querySelector(".nav-content");
  const menuLinks = document.querySelectorAll("#menu a");
  const logo = document.querySelector(".logo svg");

  // Estado inicial
  (nav as HTMLElement)?.classList.add("ceroEstilos");

  // -------------------------------------------------
  //  Funcin que decide qu clases aplicar segn scroll
  // -------------------------------------------------
  function applyScrollClasses() {
    const cur = window.scrollY;

    // 1  Ocultar/mostrar el nav al hacer scroll
    if (cur > lastScroll && cur > 200) {
      // scroll down  ocultar
      // (nav as HTMLElement)?.style.setProperty("transform", "translate(-50%, -120%)");

      nav?.classList.remove("nav-visible");
      nav?.classList.add("nav-hidden");
    } else {
      // scroll up  mostrar
      // (nav as HTMLElement)?.style.setProperty("transform", "translate(-50%, 0)");
      nav?.classList.remove("nav-hidden");
      nav?.classList.add("nav-visible");
    }

    // 2  Cambiar estilos cuando pasamos los 200px
    if (cur > 200) {
      // >200px  nav plana (sin fondo translcido)
      nav?.classList.remove("ceroEstilos");
      logo?.classList.add("logoabajo");
    } else {
      // 200px  nav con estilo cero
      nav?.classList.add("ceroEstilos");
      logo?.classList.remove("logoabajo");
    }

    lastScroll = cur;
  }

  // -------------------------------------------------
  //  Toggle del men (hamburger)
  // -------------------------------------------------
  function toggleMenu() {
    const isActive = hamburger?.classList.contains("active");

    if (!isActive) {
      // ---------- Abrir ----------
      hamburger?.classList.add("active");
      nav?.classList.add("fullscreen");

      // quitamos el estilo cero para que el men sea visible
      setTimeout(() => {
        nav?.classList.remove("ceroEstilos");
        navContent?.classList.add("active");
      }, 500);
    } else {
      // ---------- Cerrar ----------
      navContent?.classList.remove("active");

      // Esperamos a que la animacin de cierre termine
      setTimeout(() => {
        nav?.classList.remove("fullscreen");
        hamburger?.classList.remove("active");

        // 1  Resetear la transformacin que qued del fullscreen
        (nav as HTMLElement)?.style.removeProperty("transform");

        // 2  Aplicar los estilos correctos segn la posicin actual del scroll
        applyScrollClasses(); // < aqu est la clave!
      }, 200);
    }

    // Bloqueamos/desbloqueamos scroll del body
    document.body.style.overflow = isActive ? "" : "hidden";
  }

  // -------------------------------------------------
  //  Cerrar el men al hacer click en un enlace
  // -------------------------------------------------
  menuLinks.forEach((link) => {
    link.addEventListener("click", () => {
      if (navContent?.classList.contains("active")) {
        toggleMenu(); // reutilizamos la lgica anterior
      }
    });
  });

  // -------------------------------------------------
  //  Scroll  throttled con requestAnimationFrame
  // -------------------------------------------------
  let ticking = false;
  window.addEventListener("scroll", () => {
    if (!ticking) {
      window.requestAnimationFrame(() => {
        applyScrollClasses();
        ticking = false;
      });
      ticking = true;
    }
  });

  // -------------------------------------------------
  //  Listener del botn hamburguesa
  // -------------------------------------------------
  hamburger?.addEventListener("click", toggleMenu);
</script>
