    --nav-shadow: 0 6px 6px rgba(0, 0, 0, 0.2), 0 0 20px rgba(0, 0, 0, 0.1);
    --nav-margin: 0.3rem;
    --nav-border-radius: 0.8rem;
    --nav-backdrop-filter: blur(7px);

    background-color: var(--nav-bg);
    z-index: 1000;
    overflow: hidden;
    margin: var(--nav-margin);
    box-shadow: var(--nav-shadow);
    border-radius: var(--nav-border-radius);
    padding: 0 1.4rem;

    /* Transicin suave para todos los atributos */
