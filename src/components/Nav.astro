      in="SourceGraphic"
      in2="SourceGraphic"
      scale="10"
      xChannelSelector="R"
      yChannelSelector="B"
      x="0%"
      y="0%"
      width="100%"
      height="100%"
      result="displacementMap"></feDisplacementMap>

    <feGaussianBlur
      stdDeviation="3 3"
      x="0%"
      y="0%"
      width="100%"
      height="100%"
      in="displacementMap"
      edgeMode="none"
      result="blur"></feGaussianBlur>
    <feGaussianBlur in="turbulence" stdDeviation="3" result="softMap"
